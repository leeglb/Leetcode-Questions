class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """

        
        dictionary = {}
        

        for i, n in enumerate(nums): #index, value

            subtract = target - n

            if subtract in dictionary: #if the value exists in the dictionary, return the index and new index.

                return [dictionary[subtract], i]

            
            else: # otherwise set the new value to that index, we are checking for 2 seperate numbers which both exist in the list.

                dictionary[n] = i

        return 
        





a = Solution()
print(a.twoSum([3,2,4], target=6))