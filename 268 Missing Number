class Solution(object):
    def missingNumber(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        sum_1 = 0
        sum_2 = 0

        for i in nums: 
            sum_1 += i 

        for x in range(0, len(nums) + 1):

            sum_2 += x 


        return sum_2 - sum_1
        


